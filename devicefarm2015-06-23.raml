#%RAML 1.0
title: AWS Device Farm
version: '2015-06-23'
description: ' AWS Device Farm is a service that enables mobile app developers to
  test Android, iOS, and Fire OS apps on physical phones, tablets, and other devices
  in the cloud.'
mediaType:
- application/json
uses:
  commons: https://raw.githubusercontent.com/apiregistry/commons/master/commons.raml
  extras: https://raw.githubusercontent.com/apiregistry/typesExtras/master/typeExtras.raml
types:
  AWSAccountNumber:
    type: string
    minLength: 2
    maxLength: 16
  AmazonResourceName:
    type: string
    minLength: 32
  AmazonResourceNames:
    type: array
    items:
      type: AmazonResourceName
  ArtifactCategory:
    type: string
    enum:
    - SCREENSHOT
    - FILE
    - LOG
  ArtifactType:
    type: string
    enum:
    - UNKNOWN
    - SCREENSHOT
    - DEVICE_LOG
    - MESSAGE_LOG
    - VIDEO_LOG
    - RESULT_LOG
    - SERVICE_LOG
    - WEBKIT_LOG
    - INSTRUMENTATION_OUTPUT
    - EXERCISER_MONKEY_OUTPUT
    - CALABASH_JSON_OUTPUT
    - CALABASH_PRETTY_OUTPUT
    - CALABASH_STANDARD_OUTPUT
    - CALABASH_JAVA_XML_OUTPUT
    - AUTOMATION_OUTPUT
    - APPIUM_SERVER_OUTPUT
    - APPIUM_JAVA_OUTPUT
    - APPIUM_JAVA_XML_OUTPUT
    - APPIUM_PYTHON_OUTPUT
    - APPIUM_PYTHON_XML_OUTPUT
    - EXPLORER_EVENT_LOG
    - EXPLORER_SUMMARY_LOG
    - APPLICATION_CRASH_REPORT
    - XCTEST_LOG
    - VIDEO
  Artifacts:
    type: array
    items:
      type: Artifact
  BillingMethod:
    type: string
    enum:
    - METERED
    - UNMETERED
  Boolean:
    type: boolean
  ContentType:
    type: string
    minLength: 0
    maxLength: 64
  CurrencyCode:
    type: string
    enum:
    - USD
  DateTime:
    type: datetime
  DeviceAttribute:
    type: string
    enum:
    - ARN
    - PLATFORM
    - FORM_FACTOR
    - MANUFACTURER
    - REMOTE_ACCESS_ENABLED
  DeviceFormFactor:
    type: string
    enum:
    - PHONE
    - TABLET
  DevicePlatform:
    type: string
    enum:
    - ANDROID
    - IOS
  DevicePoolCompatibilityResults:
    type: array
    items:
      type: DevicePoolCompatibilityResult
  DevicePoolType:
    type: string
    enum:
    - CURATED
    - PRIVATE
  DevicePools:
    type: array
    items:
      type: DevicePool
  Devices:
    type: array
    items:
      type: Device
  Double:
    type: number
    format: double
  ExecutionResult:
    type: string
    enum:
    - PENDING
    - PASSED
    - WARNED
    - FAILED
    - SKIPPED
    - ERRORED
    - STOPPED
  ExecutionStatus:
    type: string
    enum:
    - PENDING
    - PENDING_CONCURRENCY
    - PENDING_DEVICE
    - PROCESSING
    - SCHEDULING
    - PREPARING
    - RUNNING
    - COMPLETED
    - STOPPING
  Filter:
    type: string
    minLength: 0
    maxLength: 8192
  IncompatibilityMessages:
    type: array
    items:
      type: IncompatibilityMessage
  Integer:
    type: integer
    format: int
  Jobs:
    type: array
    items:
      type: Job
  Long:
    type: integer
    format: long
  Message:
    type: string
    minLength: 0
    maxLength: 16384
  Metadata:
    type: string
    minLength: 0
    maxLength: 8192
  Name:
    type: string
    minLength: 0
    maxLength: 256
  OfferingIdentifier:
    type: string
    minLength: 32
  OfferingTransactionType:
    type: string
    enum:
    - PURCHASE
    - RENEW
    - SYSTEM
  OfferingTransactions:
    type: array
    items:
      type: OfferingTransaction
  OfferingType:
    type: string
    enum:
    - RECURRING
  Offerings:
    type: array
    items:
      type: Offering
  PaginationToken:
    type: string
    minLength: 4
    maxLength: 1024
  Problems:
    type: array
    items:
      type: Problem
  Projects:
    type: array
    items:
      type: Project
  RecurringChargeFrequency:
    type: string
    enum:
    - MONTHLY
  RecurringCharges:
    type: array
    items:
      type: RecurringCharge
  RemoteAccessSessions:
    type: array
    items:
      type: RemoteAccessSession
  RuleOperator:
    type: string
    enum:
    - EQUALS
    - LESS_THAN
    - GREATER_THAN
    - IN
    - NOT_IN
  Rules:
    type: array
    items:
      type: Rule
  Runs:
    type: array
    items:
      type: Run
  SampleType:
    type: string
    enum:
    - CPU
    - MEMORY
    - THREADS
    - RX_RATE
    - TX_RATE
    - RX
    - TX
    - NATIVE_FRAMES
    - NATIVE_FPS
    - NATIVE_MIN_DRAWTIME
    - NATIVE_AVG_DRAWTIME
    - NATIVE_MAX_DRAWTIME
    - OPENGL_FRAMES
    - OPENGL_FPS
    - OPENGL_MIN_DRAWTIME
    - OPENGL_AVG_DRAWTIME
    - OPENGL_MAX_DRAWTIME
  Samples:
    type: array
    items:
      type: Sample
  String:
    type: string
  Suites:
    type: array
    items:
      type: Suite
  TestType:
    type: string
    enum:
    - BUILTIN_FUZZ
    - BUILTIN_EXPLORER
    - APPIUM_JAVA_JUNIT
    - APPIUM_JAVA_TESTNG
    - APPIUM_PYTHON
    - APPIUM_WEB_JAVA_JUNIT
    - APPIUM_WEB_JAVA_TESTNG
    - APPIUM_WEB_PYTHON
    - CALABASH
    - INSTRUMENTATION
    - UIAUTOMATION
    - UIAUTOMATOR
    - XCTEST
    - XCTEST_UI
  Tests:
    type: array
    items:
      type: Test
  TransactionIdentifier:
    type: string
    minLength: 32
  URL:
    type: string
    minLength: 0
    maxLength: 2048
  UniqueProblems:
    type: array
    items:
      type: UniqueProblem
  UploadStatus:
    type: string
    enum:
    - INITIALIZED
    - PROCESSING
    - SUCCEEDED
    - FAILED
  UploadType:
    type: string
    enum:
    - ANDROID_APP
    - IOS_APP
    - WEB_APP
    - EXTERNAL_DATA
    - APPIUM_JAVA_JUNIT_TEST_PACKAGE
    - APPIUM_JAVA_TESTNG_TEST_PACKAGE
    - APPIUM_PYTHON_TEST_PACKAGE
    - APPIUM_WEB_JAVA_JUNIT_TEST_PACKAGE
    - APPIUM_WEB_JAVA_TESTNG_TEST_PACKAGE
    - APPIUM_WEB_PYTHON_TEST_PACKAGE
    - CALABASH_TEST_PACKAGE
    - INSTRUMENTATION_TEST_PACKAGE
    - UIAUTOMATION_TEST_PACKAGE
    - UIAUTOMATOR_TEST_PACKAGE
    - XCTEST_TEST_PACKAGE
    - XCTEST_UI_TEST_PACKAGE
  Uploads:
    type: array
    items:
      type: Upload
  AccountSettings:
    type: object
    properties:
      awsAccountNumber?:
        type: AWSAccountNumber
        description: <p>The AWS account number specified in the <code>AccountSettings</code> container.</p>
      unmeteredDevices?:
        type: PurchasedDevicesMap
        description: <p>Returns the unmetered devices you have purchased or want to purchase.</p>
      unmeteredRemoteAccessDevices?:
        type: PurchasedDevicesMap
        description: <p>Returns the unmetered remote access devices you have purchased or want to purchase.</p>
    description: <p>A container for account-level settings within AWS Device Farm.</p>
  Artifact:
    type: object
    properties:
      arn?:
        type: AmazonResourceName
        description: <p>The artifact's ARN.</p>
      name?:
        type: Name
        description: <p>The artifact's name.</p>
      type?:
        type: ArtifactType
        description: '<p>The artifact''s type.</p> <p>Allowed values include the following:</p>
          <ul> <li> <p>UNKNOWN: An unknown type.</p> </li> <li> <p>SCREENSHOT: The
          screenshot type.</p> </li> <li> <p>DEVICE_LOG: The device log type.</p>
          </li> <li> <p>MESSAGE_LOG: The message log type.</p> </li> <li> <p>RESULT_LOG:
          The result log type.</p> </li> <li> <p>SERVICE_LOG: The service log type.</p>
          </li> <li> <p>WEBKIT_LOG: The web kit log type.</p> </li> <li> <p>INSTRUMENTATION_OUTPUT:
          The instrumentation type.</p> </li> <li> <p>EXERCISER_MONKEY_OUTPUT: For
          Android, the artifact (log) generated by an Android fuzz test.</p> </li>
          <li> <p>CALABASH_JSON_OUTPUT: The Calabash JSON output type.</p> </li> <li>
          <p>CALABASH_PRETTY_OUTPUT: The Calabash pretty output type.</p> </li> <li>
          <p>CALABASH_STANDARD_OUTPUT: The Calabash standard output type.</p> </li>
          <li> <p>CALABASH_JAVA_XML_OUTPUT: The Calabash Java XML output type.</p>
          </li> <li> <p>AUTOMATION_OUTPUT: The automation output type.</p> </li> <li>
          <p>APPIUM_SERVER_OUTPUT: The Appium server output type.</p> </li> <li> <p>APPIUM_JAVA_OUTPUT:
          The Appium Java output type.</p> </li> <li> <p>APPIUM_JAVA_XML_OUTPUT: The
          Appium Java XML output type.</p> </li> <li> <p>APPIUM_PYTHON_OUTPUT: The
          Appium Python output type.</p> </li> <li> <p>APPIUM_PYTHON_XML_OUTPUT: The
          Appium Python XML output type.</p> </li> <li> <p>EXPLORER_EVENT_LOG: The
          Explorer event log output type.</p> </li> <li> <p>EXPLORER_SUMMARY_LOG:
          The Explorer summary log output type.</p> </li> <li> <p>APPLICATION_CRASH_REPORT:
          The application crash report output type.</p> </li> <li> <p>XCTEST_LOG:
          The XCode test output type.</p> </li> </ul>'
      extension?:
        type: String
        description: <p>The artifact's file extension.</p>
      url?:
        type: URL
        description: <p>The pre-signed Amazon S3 URL that can be used with a corresponding GET request to download the artifact's file.</p>
    description: <p>Represents the output of a test. Examples of artifacts include logs and screenshots.</p>
  CPU:
    type: object
    properties:
      frequency?:
        type: String
        description: <p>The CPU's frequency.</p>
      architecture?:
        type: String
        description: <p>The CPU's architecture, for example x86 or ARM.</p>
      clock?:
        type: Double
        description: <p>The clock speed of the device's CPU, expressed in hertz (Hz). For example, a 1.2 GHz CPU is expressed as 1200000000.</p>
    description: <p>Represents the amount of CPU that an app is using on a physical device.</p> <p>Note that this does not represent system-wide CPU usage.</p>
  Counters:
    type: object
    properties:
      total?:
        type: Integer
        description: <p>The total number of entities.</p>
      passed?:
        type: Integer
        description: <p>The number of passed entities.</p>
      failed?:
        type: Integer
        description: <p>The number of failed entities.</p>
      warned?:
        type: Integer
        description: <p>The number of warned entities.</p>
      errored?:
        type: Integer
        description: <p>The number of errored entities.</p>
      stopped?:
        type: Integer
        description: <p>The number of stopped entities.</p>
      skipped?:
        type: Integer
        description: <p>The number of skipped entities.</p>
    description: <p>Represents entity counters.</p>
  CreateDevicePoolRequest:
    type: object
    properties:
      projectArn:
        type: AmazonResourceName
        description: <p>The ARN of the project for the device pool.</p>
      name:
        type: Name
        description: <p>The device pool's name.</p>
      description?:
        type: Message
        description: <p>The device pool's description.</p>
      rules:
        type: Rules
        description: <p>The device pool's rules.</p>
    description: <p>Represents a request to the create device pool operation.</p>
  CreateDevicePoolResult:
    type: object
    properties:
      devicePool?:
        type: DevicePool
        description: <p>The newly created device pool.</p>
    description: <p>Represents the result of a create device pool request.</p>
  CreateProjectRequest:
    type: object
    properties:
      name:
        type: Name
        description: <p>The project's name.</p>
    description: <p>Represents a request to the create project operation.</p>
  CreateProjectResult:
    type: object
    properties:
      project?:
        type: Project
        description: <p>The newly created project.</p>
    description: <p>Represents the result of a create project request.</p>
  CreateRemoteAccessSessionConfiguration:
    type: object
    properties:
      billingMethod?:
        type: BillingMethod
        description: <p>Returns the billing method for purposes of configuring a remote access session.</p>
    description: <p>Creates the configuration settings for a remote access session, including the device model and type.</p>
  CreateRemoteAccessSessionRequest:
    type: object
    properties:
      projectArn:
        type: AmazonResourceName
        description: <p>The Amazon Resource Name (ARN) of the project for which you want to create a remote access session.</p>
      deviceArn:
        type: AmazonResourceName
        description: <p>The Amazon Resource Name (ARN) of the device for which you want to create a remote access session.</p>
      name?:
        type: Name
        description: <p>The name of the remote access session that you wish to create.</p>
      configuration?:
        type: CreateRemoteAccessSessionConfiguration
        description: <p>The configuration information for the remote access session request.</p>
    description: <p>Creates and submits a request to start a remote access session.</p>
  CreateRemoteAccessSessionResult:
    type: object
    properties:
      remoteAccessSession?:
        type: RemoteAccessSession
        description: <p>A container that describes the remote access session when the request to create a remote access session is sent.</p>
    description: <p>Represents the server response from a request to create a remote access session.</p>
  CreateUploadRequest:
    type: object
    properties:
      projectArn:
        type: AmazonResourceName
        description: <p>The ARN of the project for the upload.</p>
      name:
        type: Name
        description: <p>The upload's file name. The name should not contain the '/' character. If uploading an iOS app, the file name needs to end with the <code>.ipa</code> extension. If uploading an Android app, the file name needs to end with the <code>.apk</code> extension. For all others, the file name must end with the <code>.zip</code> file extension.</p>
      type:
        type: UploadType
        description: '<p>The upload''s upload type.</p> <p>Must be one of the following
          values:</p> <ul> <li> <p>ANDROID_APP: An Android upload.</p> </li> <li>
          <p>IOS_APP: An iOS upload.</p> </li> <li> <p>WEB_APP: A web appliction upload.</p>
          </li> <li> <p>EXTERNAL_DATA: An external data upload.</p> </li> <li> <p>APPIUM_JAVA_JUNIT_TEST_PACKAGE:
          An Appium Java JUnit test package upload.</p> </li> <li> <p>APPIUM_JAVA_TESTNG_TEST_PACKAGE:
          An Appium Java TestNG test package upload.</p> </li> <li> <p>APPIUM_PYTHON_TEST_PACKAGE:
          An Appium Python test package upload.</p> </li> <li> <p>APPIUM_WEB_JAVA_JUNIT_TEST_PACKAGE:
          An Appium Java JUnit test package upload.</p> </li> <li> <p>APPIUM_WEB_JAVA_TESTNG_TEST_PACKAGE:
          An Appium Java TestNG test package upload.</p> </li> <li> <p>APPIUM_WEB_PYTHON_TEST_PACKAGE:
          An Appium Python test package upload.</p> </li> <li> <p>CALABASH_TEST_PACKAGE:
          A Calabash test package upload.</p> </li> <li> <p>INSTRUMENTATION_TEST_PACKAGE:
          An instrumentation upload.</p> </li> <li> <p>UIAUTOMATION_TEST_PACKAGE:
          A uiautomation test package upload.</p> </li> <li> <p>UIAUTOMATOR_TEST_PACKAGE:
          A uiautomator test package upload.</p> </li> <li> <p>XCTEST_TEST_PACKAGE:
          An XCode test package upload.</p> </li> <li> <p>XCTEST_UI_TEST_PACKAGE:
          An XCode UI test package upload.</p> </li> </ul> <p> <b>Note</b> If you
          call <code>CreateUpload</code> with <code>WEB_APP</code> specified, AWS
          Device Farm throws an <code>ArgumentException</code> error.</p>'
      contentType?:
        type: ContentType
        description: <p>The upload's content type (for example, "application/octet-stream").</p>
    description: <p>Represents a request to the create upload operation.</p>
  CreateUploadResult:
    type: object
    properties:
      upload?:
        type: Upload
        description: <p>The newly created upload.</p>
    description: <p>Represents the result of a create upload request.</p>
  DeleteDevicePoolRequest:
    type: object
    properties:
      arn:
        type: AmazonResourceName
        description: <p>Represents the Amazon Resource Name (ARN) of the Device Farm device pool you wish to delete.</p>
    description: <p>Represents a request to the delete device pool operation.</p>
  DeleteDevicePoolResult:
    type: object
    description: <p>Represents the result of a delete device pool request.</p>
  DeleteProjectRequest:
    type: object
    properties:
      arn:
        type: AmazonResourceName
        description: <p>Represents the Amazon Resource Name (ARN) of the Device Farm project you wish to delete.</p>
    description: <p>Represents a request to the delete project operation.</p>
  DeleteProjectResult:
    type: object
    description: <p>Represents the result of a delete project request.</p>
  DeleteRemoteAccessSessionRequest:
    type: object
    properties:
      arn:
        type: AmazonResourceName
        description: <p>The Amazon Resource Name (ARN) of the sesssion for which you want to delete remote access.</p>
    description: <p>Represents the request to delete the specified remote access session.</p>
  DeleteRemoteAccessSessionResult:
    type: object
    description: <p>The response from the server when a request is made to delete the remote access session.</p>
  DeleteRunRequest:
    type: object
    properties:
      arn:
        type: AmazonResourceName
        description: <p>The Amazon Resource Name (ARN) for the run you wish to delete.</p>
    description: <p>Represents a request to the delete run operation.</p>
  DeleteRunResult:
    type: object
    description: <p>Represents the result of a delete run request.</p>
  DeleteUploadRequest:
    type: object
    properties:
      arn:
        type: AmazonResourceName
        description: <p>Represents the Amazon Resource Name (ARN) of the Device Farm upload you wish to delete.</p>
    description: <p>Represents a request to the delete upload operation.</p>
  DeleteUploadResult:
    type: object
    description: <p>Represents the result of a delete upload request.</p>
  Device:
    type: object
    properties:
      arn?:
        type: AmazonResourceName
        description: <p>The device's ARN.</p>
      name?:
        type: Name
        description: <p>The device's display name.</p>
      manufacturer?:
        type: String
        description: <p>The device's manufacturer name.</p>
      model?:
        type: String
        description: <p>The device's model name.</p>
      formFactor?:
        type: DeviceFormFactor
        description: '<p>The device''s form factor.</p> <p>Allowed values include:</p>
          <ul> <li> <p>PHONE: The phone form factor.</p> </li> <li> <p>TABLET: The
          tablet form factor.</p> </li> </ul>'
      platform?:
        type: DevicePlatform
        description: '<p>The device''s platform.</p> <p>Allowed values include:</p>
          <ul> <li> <p>ANDROID: The Android platform.</p> </li> <li> <p>IOS: The iOS
          platform.</p> </li> </ul>'
      os?:
        type: String
        description: <p>The device's operating system type.</p>
      cpu?:
        type: CPU
        description: <p>Information about the device's CPU.</p>
      resolution?:
        type: Resolution
      heapSize?:
        type: Long
        description: <p>The device's heap size, expressed in bytes.</p>
      memory?:
        type: Long
        description: <p>The device's total memory size, expressed in bytes.</p>
      image?:
        type: String
        description: <p>The device's image name.</p>
      carrier?:
        type: String
        description: <p>The device's carrier.</p>
      radio?:
        type: String
        description: <p>The device's radio.</p>
      remoteAccessEnabled?:
        type: Boolean
        description: <p>Specifies whether remote access has been enabled for the specified device.</p>
      fleetType?:
        type: String
        description: <p>The type of fleet to which this device belongs. Possible values for fleet type are PRIVATE and PUBLIC.</p>
      fleetName?:
        type: String
        description: <p>The name of the fleet to which this device belongs.</p>
    description: <p>Represents a device type that an app is tested against.</p>
  DeviceMinutes:
    type: object
    properties:
      total?:
        type: Double
        description: <p>When specified, represents the total minutes used by the resource to run tests.</p>
      metered?:
        type: Double
        description: <p>When specified, represents only the sum of metered minutes used by the resource to run tests.</p>
      unmetered?:
        type: Double
        description: <p>When specified, represents only the sum of unmetered minutes used by the resource to run tests.</p>
    description: <p>Represents the total (metered or unmetered) minutes used by the resource to run tests. Contains the sum of minutes consumed by all children.</p>
  DevicePool:
    type: object
    properties:
      arn?:
        type: AmazonResourceName
        description: <p>The device pool's ARN.</p>
      name?:
        type: Name
        description: <p>The device pool's name.</p>
      description?:
        type: Message
        description: <p>The device pool's description.</p>
      type?:
        type: DevicePoolType
        description: '<p>The device pool''s type.</p> <p>Allowed values include:</p>
          <ul> <li> <p>CURATED: A device pool that is created and managed by AWS Device
          Farm.</p> </li> <li> <p>PRIVATE: A device pool that is created and managed
          by the device pool developer.</p> </li> </ul>'
      rules?:
        type: Rules
        description: <p>Information about the device pool's rules.</p>
    description: <p>Represents a collection of device types.</p>
  DevicePoolCompatibilityResult:
    type: object
    properties:
      device?:
        type: Device
      compatible?:
        type: Boolean
        description: <p>Whether the result was compatible with the device pool.</p>
      incompatibilityMessages?:
        type: IncompatibilityMessages
        description: <p>Information about the compatibility.</p>
    description: <p>Represents a device pool compatibility result.</p>
  GetAccountSettingsRequest:
    type: object
    description: <p>Represents the request sent to retrieve the account settings.</p>
  GetAccountSettingsResult:
    type: object
    properties:
      accountSettings?:
        type: AccountSettings
    description: <p>Represents the account settings return values from the <code>GetAccountSettings</code> request.</p>
  GetDevicePoolCompatibilityRequest:
    type: object
    properties:
      devicePoolArn:
        type: AmazonResourceName
        description: <p>The device pool's ARN.</p>
      appArn?:
        type: AmazonResourceName
        description: <p>The ARN of the app that is associated with the specified device pool.</p>
      testType?:
        type: TestType
        description: '<p>The test type for the specified device pool.</p> <p>Allowed
          values include the following:</p> <ul> <li> <p>BUILTIN_FUZZ: The built-in
          fuzz type.</p> </li> <li> <p>BUILTIN_EXPLORER: For Android, an app explorer
          that will traverse an Android app, interacting with it and capturing screenshots
          at the same time.</p> </li> <li> <p>APPIUM_JAVA_JUNIT: The Appium Java JUnit
          type.</p> </li> <li> <p>APPIUM_JAVA_TESTNG: The Appium Java TestNG type.</p>
          </li> <li> <p>APPIUM_PYTHON: The Appium Python type.</p> </li> <li> <p>APPIUM_WEB_JAVA_JUNIT:
          The Appium Java JUnit type for Web apps.</p> </li> <li> <p>APPIUM_WEB_JAVA_TESTNG:
          The Appium Java TestNG type for Web apps.</p> </li> <li> <p>APPIUM_WEB_PYTHON:
          The Appium Python type for Web apps.</p> </li> <li> <p>CALABASH: The Calabash
          type.</p> </li> <li> <p>INSTRUMENTATION: The Instrumentation type.</p> </li>
          <li> <p>UIAUTOMATION: The uiautomation type.</p> </li> <li> <p>UIAUTOMATOR:
          The uiautomator type.</p> </li> <li> <p>XCTEST: The XCode test type.</p>
          </li> <li> <p>XCTEST_UI: The XCode UI test type.</p> </li> </ul>'
    description: <p>Represents a request to the get device pool compatibility operation.</p>
  GetDevicePoolCompatibilityResult:
    type: object
    properties:
      compatibleDevices?:
        type: DevicePoolCompatibilityResults
        description: <p>Information about compatible devices.</p>
      incompatibleDevices?:
        type: DevicePoolCompatibilityResults
        description: <p>Information about incompatible devices.</p>
    description: <p>Represents the result of describe device pool compatibility request.</p>
  GetDevicePoolRequest:
    type: object
    properties:
      arn:
